    "def bitwiseAnd(m,n):\n",
    "    if m==0:\n",
    "        return 0\n",
    "    moveFactor = 1\n",
    "    while m!=n:\n",
    "        m = m>>1\n",
    "        n = n>>1\n",
    "        moveFactor = moveFactor << 1\n",
    "    return m * moveFactor"
   ]
  },
