   "source": [
    "def rmNthfromNode(head,n):\n",
    "    fast = head\n",
    "    slow = head\n",
    "    for _ in range(n):\n",
    "        fast = fast.next\n",
    "        if fast is None:\n",
    "            return head.next\n",
    "    while fast.next:\n",
    "        fast = fast.next\n",
    "        slow = slow.next\n",
    "    \n",
    "    slow.next = slow.next.next\n",
    "    return head"
   ]
