   "source": [
    "def longestSubstring(string,k):\n",
    "    for c in set(string):\n",
    "        if string.count(c)<k:\n",
    "            return max(longestSubstring(substring,k) for substring in string.split(c))\n",
    "    else:\n",
    "        return len(string)"
   ]
  },
